{
  "kind": "collectionType",
  "collectionName": "tasks",
  "info": {
    "singularName": "task",
    "pluralName": "tasks",
    "displayName": "Task"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "external_id": {
      "type": "string",
      "configurable": false
    },
    "name": {
      "type": "string",
      "maxLength": 250,
      "required": true
    },
    "priority": {
      "type": "string",
      "default": "P3",
      "maxLength": 2,
      "required": true
    },
    "sumary": {
      "type": "string",
      "maxLength": 500,
      "required": true
    },
    "team": {
      "type": "string",
      "required": true,
      "comment": "Proviene del Item Catalogo - Padre: Catalogo Equipos DEUNA"
    },
    "incident": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::incidence.incidence",
      "required": true
    }
  },
  "hooks": {
    "beforeCreate": "lifecycles.beforeCreate",
    "beforeUpdate": "lifecycles.beforeUpdate"
  }
}
