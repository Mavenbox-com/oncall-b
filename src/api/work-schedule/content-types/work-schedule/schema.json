{
  "kind": "collectionType",
  "collectionName": "work_schedules",
  "info": {
    "singularName": "work-schedule",
    "pluralName": "work-schedules",
    "displayName": "Work schedule"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "external_id": {
      "type": "string",
      "configurable": false
    },
    "day": {
      "type": "date",
      "required": true
    },
    "day_type": {
      "type": "string",
      "maxLength": 2,
      "default": "LB",
      "required": true
    },
    "employed_work_schedules": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::employed-work-schedule.employed-work-schedule",
      "mappedBy": "schedule"
    }
  }
}
